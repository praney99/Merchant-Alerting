<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.2.0 http://maven.apache.org/xsd/settings-1.2.0.xsd" xmlns="http://maven.apache.org/SETTINGS/1.2.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <servers>
        <server>
            <id>central</id>
            <username>${ARTIFACTORY_USER}</username>
            <password>${ARTIFACTORY_TOKEN}</password>
        </server>
        <server>
            <id>snapshots</id>
            <username>${ARTIFACTORY_USER}</username>
            <password>${ARTIFACTORY_TOKEN}</password>
        </server>
    </servers>
    <proxies>
        <proxy>
            <active>true</active>
            <protocol>http</protocol>
            <host>thd-svr-proxy-qa.homedepot.com</host>
            <port>7070</port>
            <nonProxyHosts>*.homedepot.com</nonProxyHosts>
        </proxy>
    </proxies>
    <profiles>
        <profile>
            <repositories>
                <repository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>centrals</id>
                    <name>libs-release</name>
                    <url>
                        https://maven.artifactory.homedepot.com/artifactory/libs-release
                    </url>
                </repository>
                <repository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>snapshots</id>
                    <name>libs-snapshot</name>
                    <url>
                        https://maven.artifactory.homedepot.com/artifactory/libs-snapshot
                    </url>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>centrals</id>
                    <name>plugins-release</name>
                    <url>
                        https://maven.artifactory.homedepot.com/artifactory/plugins-release
                    </url>
                </pluginRepository>
                <pluginRepository>
                    <snapshots/>
                    <id>snapshots</id>
                    <name>plugins-snapshot</name>
                    <url>
                        https://maven.artifactory.homedepot.com/artifactory/plugins-snapshot
                    </url>
                </pluginRepository>
            </pluginRepositories>
            <id>artifactory</id>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>artifactory</activeProfile>
    </activeProfiles>
</settings>